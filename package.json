{
	"name": "express-starter-kit",
	"version": "1.3.0",
	"description": "Node express SaaS starter kit with postgres",
	"author": "D15 Labs (Pvt) Ltd",
	"repository": "D15-Labs/express-starter-kit",
	"license": "MIT",
	"main": "index.ts",
	"private": true,
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"scripts": {
		"build": "tsc && tsup",
		"dev": "nodemon src/index.ts",
		"start:prod": "node dist/index.js",
		"check": "biome check --write",
		"test": "vitest run",
		"test:cov": "vitest run --coverage",
		"migrate": "drizzle-kit push",
		"migrate:down": "drizzle-kit drop",
		"migrate:generate": "drizzle-kit generate",
		"studio": "drizzle-kit studio",
		"seed": "tsx src/database/seeders/index.ts",
		"prepare": "husky"
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "8.4.1",
		"bcrypt": "^6.0.0",
		"cors": "2.8.6",
		"dotenv": "17.3.1",
		"drizzle-orm": "^0.45.1",
		"express": "5.2.1",
		"helmet": "8.1.0",
		"http-status-codes": "2.3.0",
		"jsonwebtoken": "^9.0.3",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"pg": "^8.18.0",
		"pg-promise": "^12.6.1",
		"pino": "10.3.1",
		"pino-http": "11.0.0",
		"swagger-ui-express": "5.0.1",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.14",
		"@commitlint/cli": "^20.4.1",
		"@commitlint/config-conventional": "^20.4.1",
		"@types/bcrypt": "^6.0.0",
		"@types/cors": "2.8.19",
		"@types/express": "5.0.6",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/passport": "^1.0.17",
		"@types/passport-jwt": "^4.0.1",
		"@types/passport-local": "^1.0.38",
		"@types/pg": "^8.16.0",
		"@types/supertest": "6.0.3",
		"@types/swagger-ui-express": "4.1.8",
		"@vitest/coverage-v8": "3.2.4",
		"drizzle-kit": "^0.31.9",
		"husky": "^9.1.7",
		"node-pg-migrate": "^8.0.4",
		"nodemon": "^3.0.0",
		"pino-pretty": "13.1.3",
		"supertest": "7.1.4",
		"tsup": "8.5.1",
		"tsx": "4.21.0",
		"typescript": "5.9.3",
		"vite-tsconfig-paths": "6.1.1",
		"vitest": "3.2.4"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"outDir": "dist",
		"format": [
			"esm",
			"cjs"
		],
		"target": "es2020",
		"sourcemap": true,
		"clean": true,
		"dts": true,
		"splitting": false,
		"skipNodeModulesBundle": true
	},
	"packageManager": "pnpm@10.14.0"
}
